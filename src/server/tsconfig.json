{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "baseUrl": ".",
        "paths": {
            "shared/*": [
                "../shared/*"
            ]
        }
    },
    "exclude": [
        "typings"
    ]
}